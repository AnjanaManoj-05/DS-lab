#include <stdio.h>
int a[10][10], vis[10], i, j;

void Dfs(int);

void main() {
    int n;
    printf("Enter number of vertices\n");
    scanf("%d", &n);
    printf("Enter adjacency matrix\n");
    for (i = 1; i <= n; i++) {
        for (j = 1; j <= n; j++) {
            scanf("%d", &a[i][j]);
        }
    }

    for (i = 1; i <= n; i++) {
        vis[i] = 0;
    }

    printf("DFS Traversal\n");
    for (i = 1; i <= n; i++) {
        if (vis[i] == 0) {
            Dfs(i);
        }
    }
}

void Dfs(int v) {
    printf("%d ", v);
    vis[v] = 1;
    for (i = 1; i <= n; i++) {
        if (a[v][i] == 1 && vis[i] == 0) {
            Dfs(i);
        }
    }
}
